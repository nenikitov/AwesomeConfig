{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "*",
        "ts/*"
      ]
    },
    "typeRoots": [
      "./types/ags.d.ts",
      "./node_modules/@girs"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
